{
    "info": {
        "_postman_id": "symbiomed-2024",
        "name": "SymbioMed Terminology Service - SIH 2024",
        "description": "Complete API collection for SymbioMed FHIR Terminology Service. Includes all 10 endpoints for testing NAMASTE to ICD-11 TM2 mapping and FHIR resource operations.",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "variable": [
        {
            "key": "baseUrl",
            "value": "https://symbiomed.onrender.com",
            "type": "string"
        }
    ],
    "item": [
        {
            "name": "1. Health Check",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/health",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "health"
                    ]
                },
                "description": "Check if the API server is running and healthy."
            },
            "response": []
        },
        {
            "name": "2. CodeSystem $lookup",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/fhir/CodeSystem/$lookup?system=http://sih.gov.in/fhir/CodeSystem/namaste-ayurveda&code=AA",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "fhir",
                        "CodeSystem",
                        "$lookup"
                    ],
                    "query": [
                        {
                            "key": "system",
                            "value": "http://sih.gov.in/fhir/CodeSystem/namaste-ayurveda",
                            "description": "The code system URL"
                        },
                        {
                            "key": "code",
                            "value": "AA",
                            "description": "The code to look up (try: AA, AB, AC, AD)"
                        }
                    ]
                },
                "description": "Look up details for a specific NAMASTE code in the code system."
            },
            "response": []
        },
        {
            "name": "3. ValueSet $expand",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/fhir/ValueSet/$expand?url=http://sih.gov.in/fhir/ValueSet/namaste-ayurveda&filter=vata",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "fhir",
                        "ValueSet",
                        "$expand"
                    ],
                    "query": [
                        {
                            "key": "url",
                            "value": "http://sih.gov.in/fhir/ValueSet/namaste-ayurveda",
                            "description": "The ValueSet URL"
                        },
                        {
                            "key": "filter",
                            "value": "vata",
                            "description": "Search filter text (try: dosha, vata, pitta, kapha)"
                        }
                    ]
                },
                "description": "Search and expand the NAMASTE ValueSet with a filter term."
            },
            "response": []
        },
        {
            "name": "4. ConceptMap $translate",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/fhir/ConceptMap/$translate?url=urn:conceptmap:NAMASTE_TO_ICD11_TM2_MAPPING&system=urn:namaste&target=http://id.who.int/icd/entity&code=AA",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "fhir",
                        "ConceptMap",
                        "$translate"
                    ],
                    "query": [
                        {
                            "key": "url",
                            "value": "urn:conceptmap:NAMASTE_TO_ICD11_TM2_MAPPING",
                            "description": "ConceptMap URL"
                        },
                        {
                            "key": "system",
                            "value": "urn:namaste",
                            "description": "Source system"
                        },
                        {
                            "key": "target",
                            "value": "http://id.who.int/icd/entity",
                            "description": "Target system (ICD-11)"
                        },
                        {
                            "key": "code",
                            "value": "AA",
                            "description": "NAMASTE code to translate"
                        }
                    ]
                },
                "description": "Translate a NAMASTE code to ICD-11 TM2 code using ConceptMap."
            },
            "response": []
        },
        {
            "name": "5. ValueSet $validate-code (GET)",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/fhir/ValueSet/$validate-code?url=http://sih.gov.in/fhir/ValueSet/namaste-ayurveda&system=http://sih.gov.in/fhir/CodeSystem/namaste-ayurveda&code=AA",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "fhir",
                        "ValueSet",
                        "$validate-code"
                    ],
                    "query": [
                        {
                            "key": "url",
                            "value": "http://sih.gov.in/fhir/ValueSet/namaste-ayurveda",
                            "description": "ValueSet URL"
                        },
                        {
                            "key": "system",
                            "value": "http://sih.gov.in/fhir/CodeSystem/namaste-ayurveda",
                            "description": "CodeSystem URL"
                        },
                        {
                            "key": "code",
                            "value": "AA",
                            "description": "Code to validate"
                        }
                    ]
                },
                "description": "Validate if a code exists in the ValueSet (GET method)."
            },
            "response": []
        },
        {
            "name": "6. ValueSet $validate-code (POST)",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json",
                        "type": "text"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"url\": \"http://sih.gov.in/fhir/ValueSet/namaste-ayurveda\",\n  \"system\": \"http://sih.gov.in/fhir/CodeSystem/namaste-ayurveda\",\n  \"code\": \"AA\"\n}",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{baseUrl}}/fhir/ValueSet/$validate-code",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "fhir",
                        "ValueSet",
                        "$validate-code"
                    ]
                },
                "description": "Validate if a code exists in the ValueSet (POST method)."
            },
            "response": []
        },
        {
            "name": "7. Ingest Problem List",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json",
                        "type": "text"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"ayushCode\": \"AA\",\n  \"ayushSystem\": \"http://sih.gov.in/fhir/CodeSystem/namaste-ayurveda\",\n  \"clinicalStatus\": \"active\",\n  \"verificationStatus\": \"confirmed\",\n  \"encounterClass\": \"AMB\",\n  \"onsetDate\": \"2024-12-05\"\n}",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{baseUrl}}/fhir/ingest/problem-list",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "fhir",
                        "ingest",
                        "problem-list"
                    ]
                },
                "description": "Create a FHIR Condition resource (Problem List Entry) with AYUSH terminology."
            },
            "response": []
        },
        {
            "name": "8. Validate Dual Code",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json",
                        "type": "text"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"ayushCode\": \"AA\",\n  \"ayushSystem\": \"http://sih.gov.in/fhir/CodeSystem/namaste-ayurveda\"\n}",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{baseUrl}}/fhir/validate/dual-code",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "fhir",
                        "validate",
                        "dual-code"
                    ]
                },
                "description": "Validate dual coding (AYUSH + ICD-11) for a given AYUSH code."
            },
            "response": []
        },
        {
            "name": "9. Get Bundle",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/fhir/bundle",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "fhir",
                        "bundle"
                    ]
                },
                "description": "Retrieve a FHIR Bundle from the server."
            },
            "response": []
        },
        {
            "name": "10. Upload Bundle",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json",
                        "type": "text"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"resourceType\": \"Bundle\",\n  \"type\": \"collection\",\n  \"entry\": [\n    {\n      \"resource\": {\n        \"resourceType\": \"CodeSystem\",\n        \"id\": \"test-codesystem\",\n        \"url\": \"http://example.org/test-codesystem\",\n        \"status\": \"active\",\n        \"content\": \"complete\",\n        \"name\": \"TestCodeSystem\",\n        \"description\": \"A test code system for demonstration\"\n      }\n    },\n    {\n      \"resource\": {\n        \"resourceType\": \"ValueSet\",\n        \"id\": \"test-valueset\",\n        \"url\": \"http://example.org/test-valueset\",\n        \"status\": \"active\",\n        \"name\": \"TestValueSet\",\n        \"description\": \"A test value set\"\n      }\n    }\n  ]\n}",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{baseUrl}}/fhir/bundle/upload",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "fhir",
                        "bundle",
                        "upload"
                    ]
                },
                "description": "Upload a FHIR Bundle to the server."
            },
            "response": []
        }
    ]
}